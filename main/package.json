{
  "name": "xeroscout3",
  "version": "3.0.11",
  "description": "Electron based scouting program",
  "main": "dist/main.js",
  "scripts": {
    "build-renderer": "cd ../renderer && npm run build",
    "interfaces": "copyfiles -f ../renderer/src/ipc.ts src/shared",
    "tabletdb" : "copyfiles -f ../renderer/src/tabletdb.ts src/shared",
    "bundle": "copyfiles -f ../renderer/dist/renderer/xeroapp.bundle.js dist/renderer",
    "copyfiles": "npm run bundle && npm run interfaces && npm run tabletdb",
    "renderer": "npm run build-renderer && npm run copyfiles",
    "compile": "tsc",
    "main": "npm run renderer && npm run compile",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "Jack (Butch) Griffin",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^22.15.3",
    "@types/sqlite3": "^3.1.11",
    "@types/tabulator-tables": "^6.2.6",
    "babel-loader": "^10.0.0",
    "bonjour-service": "^1.3.0",
    "copy-files": "^0.1.0",
    "copyfiles": "^2.4.1",
    "electron-config": "^1.0.0",
    "electron-settings": "^4.0.4",
    "expose-loader": "^5.0.1",
    "fast-csv": "^5.0.2",
    "https": "^1.0.0",
    "mdns-js": "^0.2.9",
    "ml-matrix": "^6.12.0",
    "net": "^1.0.2",
    "papaparse": "^5.4.1",
    "shell": "^0.11.0",
    "sqlite3": "^5.1.7",
    "tabulator-tables": "^6.3.1",
    "typescript": "^5.8.3",
    "uuid": "^11.0.2",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@types/papaparse": "^5.3.15",
    "css-loader": "^7.1.2",
    "electron": "^33.0.2",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1"
  }
}
